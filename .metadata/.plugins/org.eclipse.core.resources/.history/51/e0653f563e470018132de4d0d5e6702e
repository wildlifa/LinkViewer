package model;

public class PositiveEdge extends Edge {

	public Cell topCell = null;
	public Cell bottomCell = null;
	public Cell leftCell = null;
	public Cell rightCell = null;
	
	public PositiveEdge(Model model, UpNode upNode, DownNode downNode) {
		edgeID = countID;
		countID++;	
		
		this.model = model;
		this.model.edges.add(this);
		
		this.upNode = upNode;
		this.downNode = downNode;
	}
}
